<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module package="com.axelor.gst.db" name="gst"/>
  
  <entity name="Party" >
  
	<string name="reference" title="Reference" readonly="true"/>
  	<string name="name" title="Name" required="true"/>
  	<string name="typeSelect" title="Type" required="true" selection="gst.party.type.select"/>
  	<boolean name="customer" title="Customer"/>
  	<boolean name="supplier" title="Supplier"/>
  	<string name="gstin" title="GSTIN" min="15" max="15"/>
  	
  	<one-to-many ref="com.axelor.gst.db.Contact" name="contactList" title="Contact" />
  	<one-to-many ref="com.axelor.gst.db.Address" name="addressList" title="Address"/>

  	
  </entity>
  
</domain-models>