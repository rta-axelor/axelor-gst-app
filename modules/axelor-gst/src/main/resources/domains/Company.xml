<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module package="com.axelor.gst.db" name="gst"/>
 
 
  <entity name="Company" >
  
  	<string name="name" required="true" title="Name"/>
  	
  	<many-to-one ref="com.axelor.meta.db.MetaFile" name="logo" title="Logo"/>
  	<one-to-many ref="com.axelor.gst.db.Contact" name="contactList" title="Contact"/>
  		
  	
  	
  	<string name="gstin" min="15" max="15" title="GSTIN"/>
  	
  	<one-to-many ref="com.axelor.gst.db.BankAccount" name="bankAccountList" title="Bank Details"/>
  	<many-to-one ref="com.axelor.gst.db.Address" name="address" title="Address"/>
  	
  	
  </entity>
  
</domain-models>

